{
  "name": "sweet-alert-vuetify",
  "version": "1.3.0",
  "description": "",
  "main": "dist/sweet-alert-vuetify.cjs.js",
  "browser": "dist/sweet-alert-vuetify.es.js",
  "module": "dist/sweet-alert-vuetify.es.js",
  "unpkg": "dist/sweet-alert-vuetify.iife.js",
  "exports": {
    ".": {
      "import": "./dist/sweet-alert-vuetify.es.js",
      "require": "./dist/sweet-alert-vuetify.cjs.js"
    },
    "./lib": "./src/entry.esm.js"
  },
  "author": "Marcos Andr√© Jorge <https://github.com/marcosandrejorge/>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcosandrejorge/sweet-alert-vuetify.git"
  },
  "keywords": [
    "vuetify",
    "dialog",
    "sweetalert",
    "alert",
    "modal",
    "popup",
    "confirm",
    "toast"
  ],
  "files": [
    "dist/*",
    "src/*",
    "dev/plugins/*"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vite",
    "build:es": "cross-env BUILD_FORMAT=es vite build",
    "build:iife": "cross-env BUILD_FORMAT=iife vite build",
    "build:cjs": "cross-env BUILD_FORMAT=cjs vite build",
    "build": "yarn build:es && yarn build:iife && yarn build:cjs"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "browserslist-to-esbuild": "^1.2.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.69.5",
    "vite": "^4.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.7",
    "vuetify": "^3.3.23"
  },
  "peerDependencies": {
    "vue": "^3.3.7",
    "vuetify": "^3.3.23"
  },
  "engines": {
    "node": ">=16"
  },
  "type": "module"
}
